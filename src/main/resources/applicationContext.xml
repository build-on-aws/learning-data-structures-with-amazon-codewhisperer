<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
                        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <bean id="array" class="code.buildon.aws.codewhisperer.adt.Array" />
    <bean id="matrix" class="code.buildon.aws.codewhisperer.adt.Matrix" />
    <bean id="stack" class="code.buildon.aws.codewhisperer.adt.Stack" />
    <bean id="queue" class="code.buildon.aws.codewhisperer.adt.Queue" />
    <bean id="linkedList" class="code.buildon.aws.codewhisperer.adt.LinkedList" />
    <bean id="hashTable" class="code.buildon.aws.codewhisperer.adt.HashTable" />
    <bean id="bst" class="code.buildon.aws.codewhisperer.adt.BinarySearchTree" />

    <bean id="performanceChecker" class="code.buildon.aws.codewhisperer.PerformanceChecker" />

</beans>
